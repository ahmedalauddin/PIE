{
  "env": {
    "es6": true,
    "browser": false,
    "node": true,
    "commonjs": false,
    "shared-node-browser": false,
    "worker": false,
    "amd": false,
    "mocha": false,
    "jasmine": false,
    "jest": true,
    "phantomjs": false,
    "protractor": false,
    "qunit": false,
    "jquery": false,
    "prototypejs": false,
    "shelljs": false,
    "meteor": false,
    "mongo": false,
    "applescript": false,
    "nashorn": false,
    "serviceworker": true,
    "atomtest": false,
    "embertest": false,
    "webextensions": false,
    "greasemonkey": false
  },
  "plugins": [
    "html",
    "prettier",
    "import",
    "jest",
    "json",
    "markdown",
    "eslint-comments",
    "filenames",
    "sort-class-members"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false,
      "globalReturn": true,
      "impliedStrict": true
    }
  },
  "extends": [
    "eslint:recommended",
    "prettier/standard",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "settings": {},
  "overrides": [
    {
      "files": ["*.test.js"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ],
  "rules": {
    "consistent-return": 2,
    "semi": ["error", "always"],
    "indent": ["warn", 2],
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "no-console": "warn",
    "html/indent": "+2",
    "prettier/prettier": "warn"
  }
}
